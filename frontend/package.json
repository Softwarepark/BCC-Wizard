{
  "name": "contexture",
  "scripts": {
    "css-compile-dev": "node-sass --include-path node_modules --precision 6 scss -o css/",
    "css-compile": "node-sass --include-path node_modules --output-style compressed --source-map true --source-map-contents true --precision 6 scss -o css/",
    "css-prefix": "postcss --replace css/contexture.css --use autoprefixer --map",
    "css": "npm-run-all css-compile css-prefix",
    "reload-elm": "nodetouch src/dummy.elm",
    "watch": "nodemon -e scss -x \"npm-run-all css-compile-dev reload-elm\"",
    "elm-start": "elm-live src/Main.elm --pushstate",
    "start": "npm-run-all -p elm-start watch"
  },
  "devDependencies": {
    "autoprefixer": "~10.2.5",
    "elm-live": "~4.0.2",
    "node-sass": "~5.0.0",
    "nodemon": "~2.0.7",
    "npm-run-all": "~4.1.5",
    "postcss": "~8.2.8",
    "postcss-cli": "~8.3.1",
    "touch": "~3.1.0"
  },
  "dependencies": {
    "bootstrap": "~5.0.0-beta3",
    "bootstrap-icons": "~1.4.0",
    "jquery": "~3.6.0"
  }
}
